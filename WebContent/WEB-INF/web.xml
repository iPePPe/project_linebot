<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LineBOT</display-name>
  <resource-ref>
    <res-ref-name>jdbc/DefaultDB</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
  </resource-ref>
  <resource-ref>
    <res-ref-name>TenantContext</res-ref-name>
    <res-type>com.sap.cloud.account.TenantContext</res-type>
  </resource-ref>
  <resource-ref>
    <res-ref-name>line-api</res-ref-name>
    <res-type>com.sap.core.connectivity.api.http.HttpDestination</res-type>
  </resource-ref>
  <resource-ref>
    <res-ref-name>sap-gateway-service</res-ref-name>
    <res-type>com.sap.core.connectivity.api.http.HttpDestination</res-type>
  </resource-ref>
  <resource-ref>
    <res-ref-name>KeyStoreService</res-ref-name>
    <res-type>com.sap.cloud.crypto.keystore.api.KeyStoreService</res-type>
  </resource-ref>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <resource-ref>
    <res-ref-name>connectivityConfiguration</res-ref-name>
    <res-type>com.sap.core.connectivity.api.configuration.ConnectivityConfiguration</res-type>
  </resource-ref>
  <servlet>
    <description></description>
    <display-name>CallBackServlet</display-name>
    <servlet-name>CallBackServlet</servlet-name>
    <servlet-class>com.zygen.linebot.callback.CallBackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CallBackServlet</servlet-name>
    <url-pattern>/CallBackServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CallbackHCPServlet</display-name>
    <servlet-name>CallbackHCPServlet</servlet-name>
    <servlet-class>com.zygen.linebot.callback.CallbackHCPServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CallbackHCPServlet</servlet-name>
    <url-pattern>/CallbackHCPServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ActivationServlet</display-name>
    <servlet-name>ActivationServlet</servlet-name>
    <servlet-class>com.zygen.hcp.ui.ActivationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ActivationServlet</servlet-name>
    <url-pattern>/Activation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ODataServlet</servlet-name>
    <servlet-class>org.apache.cxf.jaxrs.servlet.CXFNonSpringJaxrsServlet</servlet-class>
    <init-param>
      <param-name>javax.ws.rs.Application</param-name>
      <param-value>org.apache.olingo.odata2.core.rest.app.ODataApplication</param-value>
    </init-param>
    <init-param>
      <param-name>org.apache.olingo.odata2.service.factory</param-name>
      <param-value>com.zygen.hcp.jpa.odata.NiceODataJPAServiceFactory</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ODataServlet</servlet-name>
    <url-pattern>/Nice.svc/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GoogleMapImageServlet</display-name>
    <servlet-name>GoogleMapImageServlet</servlet-name>
    <servlet-class>com.zygen.google.client.GoogleMapImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoogleMapImageServlet</servlet-name>
    <url-pattern>/PlaceImageServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AgodaSearchBoxServlet</display-name>
    <servlet-name>AgodaSearchBoxServlet</servlet-name>
    <servlet-class>com.zygen.hcp.ui.AgodaSearchBoxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AgodaSearchBoxServlet</servlet-name>
    <url-pattern>/AgodaSearchBoxServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CallbackAiServlet</display-name>
    <servlet-name>CallbackAiServlet</servlet-name>
    <servlet-class>com.zygen.linebot.callback.CallbackAiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CallbackAiServlet</servlet-name>
    <url-pattern>/CallbackAiServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>TestAPIAIServlet</display-name>
    <servlet-name>TestAPIAIServlet</servlet-name>
    <servlet-class>com.zygen.api.ai.client.TestAPIAIServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestAPIAIServlet</servlet-name>
    <url-pattern>/TestAPIAIServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SSLExampleServlet</display-name>
    <servlet-name>SSLExampleServlet</servlet-name>
    <servlet-class>com.zygen.api.ai.client.SSLExampleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SSLExampleServlet</servlet-name>
    <url-pattern>/SSLExampleServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WorkPlaceCallback</display-name>
    <servlet-name>WorkPlaceCallback</servlet-name>
    <servlet-class>com.zygen.workplace.callback.WorkPlaceCallback</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WorkPlaceCallback</servlet-name>
    <url-pattern>/WorkPlaceCallback</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WPCallbackServlet</display-name>
    <servlet-name>WPCallbackServlet</servlet-name>
    <servlet-class>com.zygen.workplace.callback.WPCallbackServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WPCallbackServlet</servlet-name>
    <url-pattern>/WPCallback</url-pattern>
  </servlet-mapping>
</web-app>